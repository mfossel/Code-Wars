namespace ConsonantCounter
{
    class Program
    {
        public static int ConsonantCount(string str)
        {
            int count = 0;

            foreach (char c in str)
            {

                if (char.IsLetter(c) & !(c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u'))
                {
                    count += 1;
                }

            }

            return count;

        }
    }
}
